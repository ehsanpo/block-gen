<section class="cta-block{% if class %} {{class}}{% endif %}" {% if image %} style="background-image:url({{image}})" {% endif %}>
	<div class="wrapper">
		<h2>{{cta_headline}}</h2>
		<div class="box-content">{{cta_subheadline}}</div>
		<div class="itb-footer">
			<ul>
				{% for item in stn_link %}
					{% if item.link_type == 'url' %}
						<li>
							<a href="{{ item.link_url }}" class="btn">{{ item.link_text }}</a>
						</li>
					{% endif %}
					{% if item.link_type == 'page' %}
						<li>
							<a href="{{ item.link_page }}" class="btn">{{ item.link_text }}</a>
						</li>
					{% endif %}
				{% endfor %}
			</ul>
		</div>
	</div>
</section>
